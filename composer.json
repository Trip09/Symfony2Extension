{
    "name": "phpspec/symfony2-extension",
    "description": "Symfony extension for phpspec",
    "type": "library",
    "keywords": ["phpspec","symfony2"],
    "homepage": "https://github.com/phpspec/Symfony2Extension",
    "license": "MIT",
    "require": {
        "php": ">=5.3.3",
        "phpspec/phpspec": "dev-master",
        "symfony/framework-bundle": "~2.0"
    },
    "require-dev": {
        "behat/behat": "*",
        "bossa/phpspec2-expect": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "PhpSpec\\Symfony2Extension": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "authors": [
        {
            "name": "Jakub Zalas",
            "email": "jakub@zalas.pl"
        }
    ]
}
